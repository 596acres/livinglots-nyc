{
  "name": "livinglots-nyc",
  "version": "0.0.0",
  "description": "Static media for livinglots-nyc",
  "author": "Eric Brelsford",
  "license": "BSD-2-Clause",
  "main": "index.js",
  "dependencies": {
    "handlebars": "^2.0.0",
    "leaflet": "~0.7.3",
    "leaflet-pip": "^0.1.1",
    "livinglots-map": "git://github.com/596acres/livinglots-map",
    "spin.js": "~2.0.1",
    "tinymce": "^4.0.28",
    "underscore": "~1.7.0"
  },
  "devDependencies": {
    "browserify": "^4.1.10",
    "browserify-shim": "^3.6.0",
    "grunt": "^0.4.5",
    "grunt-browserify": "^2.1.0",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-cssmin": "^0.10.0",
    "grunt-contrib-handlebars": "^0.9.0",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-contrib-less": "^0.11.2",
    "grunt-contrib-watch": "^0.6.1",
    "watchify": "^1.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browser": {
    "bootstrap_button": "./bower_components/bootstrap/js/button.js",
    "bootstrap_modal": "./bower_components/bootstrap/js/modal.js",
    "bootstrap_tooltip": "./bower_components/bootstrap/js/tooltip.js",
    "fancybox": "./bower_components/fancybox/source/jquery.fancybox.js",
    "jquery.infinitescroll": "./bower_components/infinite-scroll/jquery.infinitescroll.js",
    "leaflet-active-area": "./bower_components/leaflet-active-area/src/leaflet.activearea.js",
    "leaflet.bing": "./bower_components/leaflet-plugins/layer/tile/Bing.js",
    "leaflet.dataoptions": "./bower_components/leaflet.dataoptions/src/leaflet.dataoptions.js",
    "leaflet.hash": "./bower_components/leaflet-hash/leaflet-hash.js",
    "leaflet.loading": "./bower_components/leaflet.loading/src/Control.Loading.js",
    "leaflet.usermarker": "./bower_components/leaflet.usermarker/src/leaflet.usermarker.js",
    "communist": "./bower_components/leaflet-tilelayer-vector/lib/communist.min.js",
    "AbstractWorker": "./bower_components/leaflet-tilelayer-vector/src/AbstractWorker.js",
    "CommunistWorker": "./bower_components/leaflet-tilelayer-vector/src/CommunistWorker.js",
    "TileCache": "./bower_components/leaflet-tilelayer-vector/src/TileCache.js",
    "TileLayer.GeoJSON": "./bower_components/leaflet-tilelayer-vector/src/TileLayer.GeoJSON.js",
    "TileLayer.Overzoom": "./bower_components/leaflet-tilelayer-vector/src/TileLayer.Overzoom.js",
    "TileQueue": "./bower_components/leaflet-tilelayer-vector/src/TileQueue.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "communist": "communist",
    "CommunistWorker": {
      "depends": [
        "communist:communist",
        "leaflet",
        "AbstractWorker"
      ]
    },
    "spinjs": "Spinner",
    "TileLayer.Overzoom": {
      "depends": ["TileLayer.GeoJSON"]
    },
    "TileLayer.GeoJSON": {
      "depends": [
        "CommunistWorker",
        "TileCache",
        "TileQueue"
      ]
    }
  }
}
